class Result {
    public static int sockMerchant(int n, List<Integer> ar) {
        int a = 0;
        HashSet<Integer> list = new HashSet<>();
        for(int i=0; i<n; i++){
            if(!list.contains(ar.get(i))){
                list.add(ar.get(i));
            }else{
                list.remove(ar.get(i));
                a++;
            }
        }
        return a;
    }
}
